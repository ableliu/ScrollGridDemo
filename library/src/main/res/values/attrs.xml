<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ScrollGridView">
        <attr name="text_color" format="color"/>
        <attr name="indicator_diameter" format="dimension"/>
        <attr name="indicator_selected_drawable" format="reference"/>
        <attr name="indicator_unSelect_drawable" format="reference"/>
        <attr name="indicator_left_margin" format="dimension"/>
        <attr name="indicator_right_margin" format="dimension"/>
        <attr name="indicator_position" format="integer">
            <flag name="left" value="3"/>
            <flag name="right" value="5"/>
            <flag name="center" value="17"/>
        </attr>
        <attr name="is_show_indicator" format="boolean"/>
    </declare-styleable>
</resources>